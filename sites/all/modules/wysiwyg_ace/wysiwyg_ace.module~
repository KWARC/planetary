/**
 * @file
 * wysiwyg_codemirror is an plugin for adding syntax highlighting to the tinyMCE editor
 * Zoltan Varady/FARM 2010
 */

function wysiwyg_codemirror_wysiwyg_include_directory($type) {
	switch ($type) {
		case 'editors':
		return $type;
	}
}
